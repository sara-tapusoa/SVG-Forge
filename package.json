{
  "name": "svg-forge",
  "version": "1.0.0",
  "description": "Your Figma Plugin",
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "webpack",
    "build:ts": "tsc -p tsconfig.json",
    "build:css": "unocss 'src/**/*.html' 'src/**/*.js'",
    "build:plugin": "webpack --mode production",
    "lint": "eslint --ext .ts,.tsx --ignore-pattern node_modules .",
    "lint:fix": "eslint --ext .ts,.tsx --ignore-pattern node_modules --fix ."
  },
  "author": "Sara Tapusoa",
  "license": "ISC",
  "dependencies": {
    "@octokit/rest": "^20.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@figma/eslint-plugin-figma-plugins": "^0.15.0",
    "@figma/plugin-typings": "^1.97.0",
    "@iconify-json/mdi": "^1.1.66",
    "@testing-library/jest-dom": "^5.0.0",
    "@testing-library/react": "^13.0.0",
    "@types/node": "^16.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@unocss/preset-icons": "^0.60.0",
    "@unocss/preset-typography": "^0.60.0",
    "@unocss/webpack": "^0.60.0",
    "autoprefixer": "^10.4.19",
    "css-loader": "^7.1.1",
    "eslint": "^9.7.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "html-webpack-plugin": "^5.6.0",
    "jsdom": "^24.1.0",
    "postcss-loader": "^8.1.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.3",
    "unocss": "^0.60.0",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@figma/figma-plugins/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "root": true,
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_",
          "caughtErrorsIgnorePattern": "^_"
        }
      ]
    }
  },
  "keywords": []
}
